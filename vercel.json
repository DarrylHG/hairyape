{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://apechampsswim.com.sg/:path*",
      "has": [{ "type": "host", "value": "www.apechampsswim.com.sg" }],
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://apechampsswim.com.sg" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
